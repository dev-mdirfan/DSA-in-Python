   "source": [
    "def divisible_by_29(num):\n",
    "    while (int(num / 100)) :\n",
    "        last_digit = int(num % 10)\n",
    "        num = int(num / 10)\n",
    "        num += last_digit * 3\n",
    "    return num % 29 == 0\n",
    "\n",
    "print(divisible_by_29(num))"
   ]
